{
  "editor.renderWhitespace": "none",
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "eslint.enable": true,
  "files.autoSave": "onFocusChange",
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
  },
  "search.exclude": {
    "**/node_modules": true,
  }
}
